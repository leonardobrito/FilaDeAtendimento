<p id="notice"><%= notice %></p>

<h1>Listing Tipo Consultas</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Duracao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_consultas.each do |tipo_consulta| %>
      <tr>
        <td><%= tipo_consulta.nome %></td>
        <td><%= tipo_consulta.duracao %></td>
        <td><%= link_to 'Show', tipo_consulta %></td>
        <td><%= link_to 'Edit', edit_tipo_consulta_path(tipo_consulta) %></td>
        <td><%= link_to 'Destroy', tipo_consulta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo consulta', new_tipo_consulta_path %>
